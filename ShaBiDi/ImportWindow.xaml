<Window x:Class="ShaBiDi.ImportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ImportWindow" Height="300" Width="300">
    <StackPanel>
        <Label Content="Importer les fichiers"></Label>
        <ListBox Height="200">
        </ListBox>
        <StackPanel Orientation="Horizontal">
            <Button Content="Ajouter des fichiers CSV">
            </Button>
            <Button Content="Supprimer"></Button>
            <Button Content="Fermer"></Button>
        </StackPanel>
    </StackPanel>
</Window>
